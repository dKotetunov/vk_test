<%= form_for("mail", :url => send_mess_path) do |f| %>
	<%= f.label :sender %> <%= current_user.email %> <br>
	<%= f.label :subject %> <%= f.text_field :subject %><br>
	<%= f.label :Message_body %> <%= f.text_field :body %><br>
	<%= f.label :Received %> <%= f.text_field :res %><br>
	<%= f.submit "Send" %>
<% end %>